{
  "name": "arya-backend",
  "version": "1.0.0",
  "description": "ARYA Medical Assistant Backend - Emergency Medicine Knowledge Base",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "wikem:import": "ts-node src/sources/wikem/importWikiEM.ts",
    "wikem:import:chest-pain": "ts-node src/sources/wikem/importWikiEM.ts --topics 'Chest_pain' --activate",
    "wikem:import:stroke": "ts-node src/sources/wikem/importWikiEM.ts --topics 'Stroke' --activate",
    "wikem:import:sepsis": "ts-node src/sources/wikem/importWikiEM.ts --topics 'Sepsis' --activate",
    "wikem:import:core": "ts-node src/sources/wikem/importWikiEM.ts --topics 'Chest_pain,Stroke,Sepsis,Shortness_of_breath,Abdominal_pain' --activate",
    "wikem:import:all": "ts-node src/sources/wikem/importWikiEM.ts --topics 'Chest_pain,Stroke,Sepsis,Shortness_of_breath,Abdominal_pain,Headache,Dizziness,Syncope,Seizure,Trauma' --activate",
    "wikem:cache:clear": "ts-node -e \"import('./src/sources/wikem/scrape').then(m => m.wikiEMScraper.clearCache())\"",
    "wikem:cache:stats": "ts-node -e \"import('./src/sources/wikem/scrape').then(m => m.wikiEMScraper.getCacheStats().then(console.log))\"",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist var/cache var/rulepacks",
    "setup": "npm install",
    "setup:dirs": "mkdir var\\cache\\wikem var\\rulepacks",
    "setup:full": "npm run setup && npm run setup:dirs",
    "test:diagnostics": "node test-diagnostics.js",
    "preflight": "node -e \"require('./src/config/preflight').runPreflightChecks({strict: true})\"",
    "test:startup": "npm run preflight && npm start"
  },
  "keywords": [
    "emergency-medicine",
    "medical-assistant",
    "rule-engine",
    "wikem",
    "arya",
    "healthcare"
  ],
  "author": "ARYA Health Team",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/speech": "^7.2.0",
    "@google-cloud/text-to-speech": "^6.2.0",
    "@google-cloud/translate": "^9.2.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "openai": "^5.12.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.0",
    "uuid": "^11.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arya-health/arya-backend.git"
  },
  "bugs": {
    "url": "https://github.com/arya-health/arya-backend/issues"
  },
  "homepage": "https://github.com/arya-health/arya-backend#readme"
}
